<section class="blog-4">
  <div class="container">
      <% for (var i =0; i < posts.length; i++) { %>
      <div class="row">
          <div class="col-sm-7">
            <h3><%= posts[i].getText('blog.title') %></h3>
            <%- posts[i].getStructuredText('blog.body').asHtml(context) %>
              <div class="tags">
                  <% if (posts[i].tags.length > 0) { %>
                    <ul>
                      <% for (var j=0; j < posts[i].tags.length > 0; j++) { %>
                        <li><%= posts[i].tags[j] %></li>
                      <% } %>
                    </ul>
                  <% } %>
              </div>
            </div>
